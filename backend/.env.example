# Example environment variables for local development
# Copy to .env and modify values as needed before running migrations or starting the app

# MySQL connection (used by runMigrations and db.service)
DB_HOST=mysql          # set to 'mysql' when running via docker compose, or 127.0.0.1 when on host
DB_PORT=3306
DB_USER=root
DB_PASSWORD=rootpw
DB_NAME=raydb

# App
NODE_ENV=development
PORT=80
JWT_SECRET=dev-secret

# (Optional) AWS / RDS values (not used for local mysql)
AWS_REGION=
DB_CLUSTER_ARN=
DB_SECRET_ARN=
